<div class="memo">
  <div class="side_bar_area">
    <div class="side_bar_area_child">
      <% @memos.each do |memo| %>
        <div class="book_name">
          <%= memo.name %>
        </div>
      <% end %>
    </div>
  </div>
  <div class="main_area">
    <div class="memo_btn_area">
        <%= link_to "編集", memos_path, class: :memo_edit_btn %>
        <%= link_to "削除", memos_path, class: :memo_destroy_btn %>
    </div>
    <%= render "main_area" %>
  </div>
</div>